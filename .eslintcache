n          <Route path={\"/\"}>\n            <HomePage />\n          </Route>\n\n          <Route>\n            <NotFound />\n          </Route>\n        </Switch>\n      </Loading>\n    </Router>\n  );\n}\n\nexport default connect(\n  (state) => ({ token: state.token }),\n  (dispatch) => ({\n    updateToken: (token) => dispatch(action.TOKEN.update(token)),\n  })\n)(App);\n","/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/component/signUp/index.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/component/forgotPassword/index.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/component/signIn/index.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/component/notFound/index.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/config.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/component/signIn/apiService.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/component/signIn/style.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/apiClient/URL.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/component/header/index.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/apiClient/index.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/storage/index.js",[],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCode/Customer/caro-client/src/component/game/index.js",["366","367","368"],"/Volumes/DATA/Education/2020_2021/Semester_1/Web_Advanced/DACK/SourceCod